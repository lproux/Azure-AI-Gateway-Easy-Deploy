    "    print('[OK] Step 1 complete')\n",
    "\n",
    "print()\n",
    "print('[OK] Step 1 outputs retrieved:')\n",
    "print(f'  - APIM Gateway: {step1_outputs.get(\"apimGatewayUrl\", \"N/A\")}')\n",
