    "### Lab 03: Logging Test 1 - Log Analytics Query"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generate logs for test 1\n",
    "for i in range(5):\n",
    "    client.chat.completions.create(\n",
    "        model='gpt-4o-mini',\n",
    "        messages=[{'role': 'user', 'content': f'Log test {i}'}],\n",
    "        max_tokens=5\n",
    "    )\n",
    "print(f'Log test 1: 5 requests logged')\n"
   ]
  },
